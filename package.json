        "babel-eslint": "^10.1.0",
        "eslint": "^7.16.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.21.5",
        "husky": "^4.3.6",
        "lint-staged": "^10.5.2",
        "prettier": "^2.2.1"
